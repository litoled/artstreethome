<h2>Sign up</h2>

<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>

  <%= form_tag(stations_show_path, method: "get") do %>
  <%= label_tag(:q, "Entrez votre station :") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag 'Go' %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :user_first_name, required: true %>
    <%= f.text_field :user_first_name, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :user_last_name, required: true %>
    <%= f.text_field :user_last_name, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.text_field :password, required: true %>
  </div>

 <div class="field">
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation, required: true %>
  </div>

  <div class="field">
    <%= f.label :artist_name %><br />
    <%= f.email_field :artist_name %>
  </div>

  <div class="field">
    <%= f.label :photo %><br />
    <%= f.email_field :photo %>
  </div>

  <div class="field">
    <%= f.label :phone_number %><br />
    <%= f.email_field :phone_number %>
  </div>

  <div class="field">
    <%= f.label :rib %><br />
    <%= f.email_field :rib %>
  </div>

  <div class="field">
    <%= f.label :artist_name %><br />
    <%= f.email_field :artist_name %>
  </div>

  <div class="field">
