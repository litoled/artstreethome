  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.text_field :password, required: true %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.text_field :password, required: true %>
  </div>









  <div class="form-actions">
    <%= f.button :submit, "Sign up" %>
  </div>
<% end %>

